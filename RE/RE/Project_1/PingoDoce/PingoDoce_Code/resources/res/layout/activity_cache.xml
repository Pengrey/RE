<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:tag="layout/activity_cache_0" android:layout_width="match_parent" android:layout_height="wrap_content">
    <Spinner android:id="@+id/spinner" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="16dp" app:layout_constraintTop_toTopOf="0"/>
    <com.google.android.material.button.MaterialButton android:theme="@style/App.Button" android:id="@+id/fetchbutton" android:layout_width="match_parent" android:layout_height="64dp" android:layout_marginTop="32dp" android:text="Fetch new Values" app:layout_constraintTop_toBottomOf="@+id/spinner" style="@style/App.WidgetStyle.Button"/>
    <com.google.android.material.button.MaterialButton android:theme="@style/App.Button" android:id="@+id/clearbutton" android:layout_width="match_parent" android:layout_height="64dp" android:text="Clear" app:layout_constraintTop_toBottomOf="@+id/fetchbutton" style="@style/App.WidgetStyle.Button"/>
    <TextView android:id="@+id/description" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/clearbutton"/>
</androidx.constraintlayout.widget.ConstraintLayout>
